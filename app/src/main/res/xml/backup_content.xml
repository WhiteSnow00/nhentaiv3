<?xml version="1.0" encoding="utf-8"?>
<full-backup-content>
    <!--
    ./adb shell bmgr fullbackup com.maxwai.nclientv3
    ./adb shell bmgr restore com.maxwai.nclientv3
    -->
    <include
        domain="sharedpref"
        path="." />

    <!-- Contains persisted session cookies / auth state (PersistentCookieJar via SharedPrefsCookiePersistor). -->
    <exclude
        domain="sharedpref"
        path="Login.xml" />
</full-backup-content>
