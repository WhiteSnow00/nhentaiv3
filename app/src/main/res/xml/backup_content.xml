<?xml version="1.0" encoding="utf-8"?>
<full-backup-content>
    <!--
    ./adb shell bmgr fullbackup com.maxwai.nclientv3
    ./adb shell bmgr restore com.maxwai.nclientv3
    -->
    <include
        domain="sharedpref"
        path="." />
    <!-- Do not back up persisted cookies / session state (CookiePersistor uses SharedPreferences "Login"). -->
    <exclude
        domain="sharedpref"
        path="Login.xml" />
</full-backup-content>
